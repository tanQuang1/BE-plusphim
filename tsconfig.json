{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "sourceMap": true,
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./api/v1/@types", "./node_modules/@types"]
  },
  "include": ["."],
  "exclude": ["node_modules"]
}
